  "description": "iciba划词翻译",
  "main": "server.js",
  "scripts": {
    "build": "npm run webpack:prod || ts-node -P tsconfig-util.json ./utils/addMetaData.ts && npm run clean",
    "watch": "nodemon --config ./config/nodemon.json --exec \"npm run dev || echo 'build finished'\"",
    "dev": "npm run webpack:dev",
    "a": "npm run analyze",
    "analyze": "webpack --config ./config/webpack.prod.analyze.conf.js",
    "webpack:dev": "webpack --config ./config/webpack.dev.conf.js",
    "webpack:prod": "webpack --config ./config/webpack.prod.conf.js",
    "clean": "ts-node -P tsconfig-util.json ./utils/apiBuildCleanup.ts",
    "lint": "node ./tslintRules/compile.js && tslint -p ./tsconfig.json ./src/** ./config/** | ts-node -P tsconfig-util.json utils/tslintFormatter.ts",
    "lint:css": "stylelint ./src"
  },
  "keywords": [],
  "author": "noe132",
  "license": "ISC",
