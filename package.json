    "cheerio": "^1.0.0-rc.2",
    "google-translate-token": "^1.0.0",
    "iconv-lite": "^0.4.21",
    "less": "^3.0.1",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^6.0.0",
    "webpack-bundle-analyzer": "^2.11.1"
  },
