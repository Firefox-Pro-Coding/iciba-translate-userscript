    "@types/greasemonkey": "^4.0.1",
    "@types/jest": "^26.0.22",
    "@types/md5": "^2.3.0",
    "@types/node": "^14.14.37",
    "@types/rimraf": "^3.0.0",
    "@types/tampermonkey": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
